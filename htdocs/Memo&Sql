create table user(){
  u_num number primary key not null,
  u_id varchar2(100),
  u_password varchar2(200)
};

create table coupon(){
  c_num number primary key not null,
  c_counum varchar2(200),  # 쿠폰의 번호
  c_group varchar2(200),   #그룹
  c_used varchar2(20)    #0과 1로 구분 
};


create table usedcoupon(){
  r_num number primary key not null,
  c_num number reference to coupon(c_num),  # 쿠폰 테이블과 연결을 하기 위한 부분
  r_usedid varchar2(200),  # 쿠폰 사용한 아이디
  r_usedate varchar2(200),   #사용날짜
};

오라클 연동에 실패해 사용을 하려고 했던 DB를 여기에 적어뒀습니다.
